<section class="m-wrap bg-muted-light">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h1 class="pb-5">{{ title }} <a href="#" type="button" class="h-btn h-btn-link-primary float-end my-3">{{ link_viewmore }}</a></h1>
        <div class="newsList newsList__slide">
          {% if items %}
            {% for item in items %}
              {{ items }}
            {% endfor %}
          {% else %}
            {% for data in news.datas %}
              {% include 'molecules/cards/news.twig' with {
                "title": data.title,
                "entity": data.entity,
                "tag": data.tag,
                "img": data.img,
                "date": data.date,
                "url" : data.url
              } %}
            {% endfor %}
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</section>
