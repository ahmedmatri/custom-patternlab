<div class="newsList__item">
  <a href={{ url}} target="_blank">
    <div class="newsList__item--img">
      {{ img }}
    </div>
    {% if tag %}<div class="newsList__item--tag">{{ tag }}</div> {% endif %}
    {% if entity %}   <div class="newsList__item--entity">{{ entity }}</div> {% endif %}
    <h4 class="newsList__item--title">{{ title }}</h4>
    <h5 class="newsList__item--date">{{ date }}</h5>
  </a>
</div>
